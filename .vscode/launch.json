{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "name": "Launch application",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "node_modules/**"
      ]
    },
    {
      "type": "firefox",
      "name": "Attach to application",
      "request": "attach",
      "skipFiles": [
        "node_modules/**"
      ]
    }
  ]
}
